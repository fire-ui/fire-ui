{
  "name": "@fire-ui/fire-ui",
  "version": "0.2.7",
  "description": "The framework contains everything that is frequently used on the frontend",
  "main": "FireUI.js",
  "scripts": {
    "compile-main-scss": "npx node-sass FireUISource.scss FireUI.css",
    "minify-main-css": "npx minify FireUI.css > FireUI.min.css",
    "compile-all-scss": "npx node-sass -o src/feature src/feature/ && npm run compile-main-scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimlimjustin/fire-ui.git"
  },
  "keywords": [
    "css",
    "framework",
    "css-framework"
  ],
  "author": "Justin Maximillian Kimlim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kimlimjustin/fire-ui/issues"
  },
  "homepage": "https://github.com/kimlimjustin/fire-ui#readme",
  "devDependencies": {
    "minify": "^7.0.0",
    "node-sass": "^5.0.0"
  }
}
